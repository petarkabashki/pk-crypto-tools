{
    "exchanges": {
        "binance": {
            "enabled": true,
            "cred": {
                "apiKey": "$binance_apiKey",
                "secret": "$binance_apiSecret"
            } 
        },
        "kucoin": {
            "enabled": true,
            "cred": {
                "apiKey": "$kucoin_apiKey",
                "secret": "$kucoin_apiSecret",
                "password": "$kucoin_apiPassphrase"
            }
        },
        "ftx": {
            "enabled": true,
            "cred": {
                "apiKey": "$ftx_apiKey",
                "secret": "$ftx_apiSecret"
            }
        }
    }
} 